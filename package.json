{
  "name": "senioangel-bootstrap",
  "version": "1.0.0",
  "description": "Ein modernes und responsives Bootstrap-Projekt fÃ¼r die SenioAngel-Plattform",
  "main": "index.js",
  "scripts": {
    "build": "sass scss/bootstrap.scss:build/css/bootstrap.css --style=expanded --source-map",
    "build:min": "sass scss/bootstrap.scss:build/css/bootstrap.min.css --style=compressed --source-map",
    "watch": "sass --watch scss:build/css --style=expanded --source-map",
    "dev": "npm run watch",
    "clean": "rm -rf build/css/*.css build/css/*.css.map",
    "serve": "live-server --port=3000 --open=/build"
  },
  "keywords": [
    "bootstrap",
    "scss",
    "responsive",
    "senioangel",
    "css-framework"
  ],
  "author": "SenioAngel Team",
  "license": "MIT",
  "devDependencies": {
    "live-server": "^1.2.0",
    "sass": "^1.69.0"
  },
  "dependencies": {
    "bootstrap": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
